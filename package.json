{
    "name": "@budarin/pluggable-serviceworker",
    "version": "1.5.0",
    "description": "Extensible via plugins service worker",
    "keywords": [
        "serviceworker",
        "plugins",
        "extensible",
        "serviceworker-plugins",
        "serviceworker-plugins-extensible"
    ],
    "author": "Vadim Budarin",
    "license": "MIT",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
